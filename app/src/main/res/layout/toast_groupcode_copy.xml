<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cstlay_groupcode_toast"
    android:layout_width="312dp"
    android:layout_height="68dp"
    android:background="@drawable/rect_white_31dp"
    android:elevation="5dp">


    <ImageView
        android:id="@+id/iv_groupcode_copy_fist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:src="@drawable/ic_fist"
        app:layout_constraintBottom_toBottomOf="@+id/tv_groupcode_copy"
        app:layout_constraintStart_toEndOf="@+id/tv_groupcode_copy"
        app:layout_constraintTop_toTopOf="@+id/tv_groupcode_copy" />

    <TextView
        android:id="@+id/tv_groupcode_copy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/notosanscjkkrbold"
        android:includeFontPadding="false"
        android:text="클립보드에 저장 완료"
        android:textColor="@color/colorBlack28"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>

