<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/tv_first_login_withoutgroupcode3_title_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/notosanscjkkrbold"
        android:lineSpacingExtra="15sp"
        android:includeFontPadding="false"
        android:text="조직이"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_first_login_withoutgroupcode3_title_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:includeFontPadding="false"
        android:fontFamily="@font/notosanscjkkrbold"
        android:lineSpacingExtra="15sp"
        android:text="생성되었습니다!"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_first_login_withoutgroupcode3_title_1" />

    <ImageView
        android:layout_width="29dp"
        android:layout_height="29dp"
        android:layout_marginStart="6dp"
        android:src="@drawable/ic_group"
        app:layout_constraintBottom_toBottomOf="@id/tv_first_login_withoutgroupcode3_title_2"
        app:layout_constraintStart_toEndOf="@id/tv_first_login_withoutgroupcode3_title_2"
        app:layout_constraintTop_toTopOf="@id/tv_first_login_withoutgroupcode3_title_2" />

    <TextView
        android:id="@+id/tv_first_login_withoutgroupcode3_sub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/notosanscjkkrmedium"
        android:includeFontPadding="false"
        android:lineSpacingExtra="8sp"
        android:text="코드를 보내 직원들을 초대하세요"
        android:textColor="#a3a3a3"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@id/tv_first_login_withoutgroupcode3_title_2"
        app:layout_constraintTop_toBottomOf="@id/tv_first_login_withoutgroupcode3_title_2" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_first_login_withoutgroupcode3_sub">

        <TextView
            android:id="@+id/tv_first_login_withoutgroupcode3_copycode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:lineSpacingExtra="6sp"
            android:includeFontPadding="false"
            android:text="조직 코드 전달"
            android:textColor="#707070"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/edtxt_first_login_withoutgroupcode3_copycode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:background="@null"
            android:fontFamily="@font/gilroyextrabold"
            tools:text="G0193NpC2"
            android:includeFontPadding="false"
            android:lineSpacingExtra="6sp"
            android:textColor="#282828"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_first_login_withoutgroupcode3_copycode" />

        <ImageButton
            android:id="@+id/imgbtn_first_login_withoutgroupcode3_copycode"
            android:layout_width="33dp"
            android:layout_height="38dp"
            android:background="@null"
            android:paddingStart="5dp"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:src="@drawable/ic_copy"
            app:layout_constraintBottom_toBottomOf="@id/lv_first_login_withoutgroupcode3"
            app:layout_constraintEnd_toEndOf="parent" />

        <View
            android:id="@+id/lv_first_login_withoutgroupcode3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="7.7dp"
            android:background="#ededed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/edtxt_first_login_withoutgroupcode3_copycode" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cstlay_group_creation_toast"
        android:layout_width="312dp"
        android:layout_height="62dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/rect_white_31dp"
        android:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>