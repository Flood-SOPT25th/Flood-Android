<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/iv_dialog_custom_signup"
        android:layout_width="312dp"
        android:layout_height="181dp"
        app:srcCompat="@drawable/rect_white_21dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cstlay_signup_alert_dialog"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/iv_dialog_custom_signup"
        app:layout_constraintBottom_toBottomOf="@id/iv_dialog_custom_signup"
        app:layout_constraintStart_toStartOf="@id/iv_dialog_custom_signup"
        app:layout_constraintEnd_toEndOf="@id/iv_dialog_custom_signup">

        <TextView
            android:id="@+id/tv_signup_alert_msg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/notosanscjkkrbold"
            android:text="앗, 아직 다 입력하지 않았어요!"
            android:textColor="#282828"
            app:layout_constraintBottom_toTopOf="@id/iv_dialog_custom_signup_blue"
            app:layout_constraintEnd_toEndOf="@+id/cstlay_signup_alert_dialog"
            app:layout_constraintHorizontal_bias="0.4"
            app:layout_constraintStart_toStartOf="@id/cstlay_signup_alert_dialog"
            app:layout_constraintTop_toTopOf="@id/cstlay_signup_alert_dialog" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            app:layout_constraintBottom_toBottomOf="@id/tv_signup_alert_msg"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@id/tv_signup_alert_msg"
            app:layout_constraintTop_toTopOf="@id/tv_signup_alert_msg"
            app:srcCompat="@drawable/ic_embarrassed" />

        <ImageView
            android:id="@+id/iv_dialog_custom_signup_blue"
            android:layout_width="277dp"
            android:layout_height="47dp"
            app:layout_constraintBottom_toBottomOf="@id/cstlay_signup_alert_dialog"
            app:layout_constraintEnd_toEndOf="@id/cstlay_signup_alert_dialog"
            app:layout_constraintStart_toStartOf="@id/cstlay_signup_alert_dialog"
            app:layout_constraintTop_toTopOf="@id/cstlay_signup_alert_dialog"
            app:layout_constraintVertical_bias="0.76"
            app:srcCompat="@drawable/rect_blue_24dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="알겠습니다"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="@id/iv_dialog_custom_signup_blue"
            app:layout_constraintEnd_toEndOf="@id/iv_dialog_custom_signup_blue"
            app:layout_constraintStart_toStartOf="@id/iv_dialog_custom_signup_blue"
            app:layout_constraintTop_toTopOf="@id/iv_dialog_custom_signup_blue" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>