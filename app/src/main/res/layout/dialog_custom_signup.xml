<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >

    <ImageView
        android:id="@+id/iv_dialog_custom_signup"
        android:layout_width="312dp"
        android:layout_height="181dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rect_white_21dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="앗, 아직 다 입력하지 않았어요!"
        android:textColor="#282828"
        android:fontFamily="@font/notosanscjkkrbold"
        app:layout_constraintBottom_toTopOf="@id/iv_dialog_custom_signup_blue"
        app:layout_constraintEnd_toEndOf="@+id/iv_dialog_custom_signup"
        app:layout_constraintStart_toStartOf="@id/iv_dialog_custom_signup"
        app:layout_constraintTop_toTopOf="@id/iv_dialog_custom_signup" />

    <ImageView
        android:id="@+id/iv_dialog_custom_signup_blue"
        android:layout_width="277dp"
        android:layout_height="47dp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_dialog_custom_signup"
        app:layout_constraintEnd_toEndOf="@+id/iv_dialog_custom_signup"
        app:layout_constraintStart_toStartOf="@+id/iv_dialog_custom_signup"
        app:layout_constraintTop_toTopOf="@id/iv_dialog_custom_signup"
        app:layout_constraintVertical_bias="0.76"
        app:srcCompat="@drawable/rect_blue_24dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="알겠습니다"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/iv_dialog_custom_signup_blue"
        app:layout_constraintEnd_toEndOf="@id/iv_dialog_custom_signup_blue"
        app:layout_constraintStart_toStartOf="@id/iv_dialog_custom_signup_blue"
        app:layout_constraintTop_toTopOf="@id/iv_dialog_custom_signup_blue" />

</androidx.constraintlayout.widget.ConstraintLayout>